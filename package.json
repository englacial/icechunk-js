{
  "name": "@englacial/icechunk-js",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "TypeScript read-only client for Icechunk stores, compatible with zarrita.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "flatbuffers": "^24.3.25",
    "fzstd": "^0.1.1"
  },
  "peerDependencies": {
    "zarrita": ">=0.4.0"
  },
  "peerDependenciesMeta": {
    "zarrita": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0",
    "prettier": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/englacial/icechunk-js.git"
  },
  "keywords": [
    "icechunk",
    "zarr",
    "zarrita",
    "tensor",
    "ndarray",
    "cloud-storage"
  ],
  "author": "Shane Grigsby",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false
}
